<AnchorPane fx:id="ap_root" styleClass="transparent" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.uninsubria.centrivaccinali.controller.CIDashboardController">
   <AnchorPane prefHeight="800.0" prefWidth="1200.0" styleClass="default-bg">
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
   </AnchorPane>
   <AnchorPane layoutX="-6.0" layoutY="1.0" prefHeight="100.0" AnchorPane.bottomAnchor="700.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <Label alignment="CENTER" styleClass="label-titleDashboard" text="AREA CITTADINO" textFill="#323232" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="27.0">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <MenuButton fx:id="mb_utente" layoutX="836.0" layoutY="15.0" mnemonicParsing="false" styleClass="MenuButton" text="utente di prova" visible="false" AnchorPane.rightAnchor="56.0" AnchorPane.topAnchor="16.0">
         <items>
            <MenuItem fx:id="ItemInfo" mnemonicParsing="false" onAction="#showInfoMB" text="Info" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="ItemLogout" mnemonicParsing="false" onAction="#logoutInfoMB" text="Logout" />
         </items>
         <graphic>
            <FontIcon iconColor="#323232" iconLiteral="mdi2a-account" iconSize="22" styleClass="iconUserConnection" />
         </graphic>
         <font>
            <Font size="14.0" />
         </font>
      </MenuButton>
      <VBox fx:id="vb_free" alignment="CENTER" layoutX="913.0" layoutY="9.0" spacing="3.0" AnchorPane.rightAnchor="56.0" AnchorPane.topAnchor="12.0">
         <HBox spacing="16.0" styleClass="contorno-opaco-50">
            <padding>
               <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
            </padding>
            <TextField fx:id="tf_loginUsername" focusTraversable="false" prefHeight="27.0" prefWidth="85.0" promptText="Username" styleClass="field-preset-1">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <PasswordField fx:id="tf_loginPassword" focusTraversable="false" prefHeight="27.0" prefWidth="85.0" promptText="Password" styleClass="field-preset-1">
               <font>
                  <Font size="14.0" />
               </font>
            </PasswordField>
            <Button fx:id="b_login" onAction="#loginDash" styleClass="button-preset-1" text="Login">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </HBox>
         <Button onAction="#loginDash" styleClass="transparent" text="Non hai un account?  Registrati" underline="true" />
      </VBox>
      <ImageView fitHeight="24.0" fitWidth="24.0" layoutX="1058.0" layoutY="-36.0" onMouseClicked="#chiudi" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="16.0" AnchorPane.topAnchor="16.0">
      </ImageView>
   </AnchorPane>
   <Pane fx:id="p_container" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0">
      <styleClass>
         <String fx:value="opaco-20" />
         <String fx:value="bordo-sotto" />
      </styleClass>
   </Pane>
   <FontIcon iconColor="#323232" iconLiteral="mdi2r-reply" iconSize="30" onMouseClicked="#backTo" styleClass="redIcon" AnchorPane.leftAnchor="16.0" AnchorPane.topAnchor="16.0" />
   <FontIcon iconColor="#323232" iconLiteral="mdi2c-close-thick" iconSize="30" onMouseClicked="#chiudi" styleClass="redIcon" AnchorPane.rightAnchor="16.0" AnchorPane.topAnchor="16.0" />
   <stylesheets>
      <URL value="@../style/style.css" />
      <URL value="@../style/dashboardStyle.css" />
   </stylesheets>
</AnchorPane>
