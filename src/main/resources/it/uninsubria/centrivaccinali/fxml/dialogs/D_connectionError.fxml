<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<DialogPane minHeight="200.0" minWidth="300.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <Label alignment="CENTER" prefHeight="69.0" prefWidth="382.0" text="Connessione al server assente, vuoi provare a connetterti?">
         <font>
            <Font size="14.0" />
         </font></Label>
   </content>
   <buttonTypes>
      <ButtonType fx:constant="YES" />
      <ButtonType fx:constant="CLOSE" />
   </buttonTypes>
   <header>
      <Label alignment="CENTER" contentDisplay="RIGHT" text=" Errore di connessione     ">
         <font>
            <Font size="24.0" />
         </font>
         <graphic>
            <FontIcon iconColor="#eb2525" iconLiteral="fltfal-error-circle-12" />
         </graphic></Label>
   </header>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</DialogPane>
