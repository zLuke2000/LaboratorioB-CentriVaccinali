<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<VBox prefHeight="580.0" prefWidth="600.0" stylesheets="@style/style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.uninsubria.centrivaccinali.controller.CVRegistraCittadinoController">
   <children>
      <AnchorPane prefHeight="52.0" prefWidth="600.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutY="11.0" styleClass="labelWelcome" text="REGISTRA CITTADINO VACCINATO" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
            <FontIcon fx:id="FI_CV_back" iconLiteral="subway-reply" iconSize="25" layoutX="14.0" layoutY="35.0" onMouseClicked="#BackTo" styleClass="backIcon" />
         </children>
      </AnchorPane>
      <VBox alignment="TOP_CENTER" prefHeight="88.0" prefWidth="600.0">
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
         <children>
            <Label text="Dati Centro Vaccinale" />
            <TextField fx:id="TF_CV_centroVaccinale" promptText="Inseriscinome centro vaccinale...">
               <VBox.margin>
                  <Insets left="70.0" right="70.0" top="10.0" />
               </VBox.margin>
            </TextField>
         </children>
      </VBox>
      <Separator opacity="0.2" prefWidth="200.0">
         <effect>
            <InnerShadow />
         </effect>
      </Separator>
      <VBox alignment="TOP_CENTER" prefHeight="132.0" prefWidth="600.0">
         <padding>
            <Insets top="10.0" />
         </padding>
         <children>
            <Label text="Dati anagrafici cittadino vaccinato">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Label>
            <HBox alignment="CENTER" prefHeight="58.0" prefWidth="600.0">
               <padding>
                  <Insets left="70.0" right="70.0" />
               </padding>
               <children>
                  <TextField fx:id="TF_CV_nomeCittadino" prefHeight="27.0" prefWidth="275.0" promptText="Nome">
                     <HBox.margin>
                        <Insets right="5.0" />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="TF_CV_cognomeCittadino" prefHeight="27.0" prefWidth="275.0" promptText="Cognome">
                     <HBox.margin>
                        <Insets left="50.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </HBox>
            <TextField fx:id="TF_CV_cfCittadino" promptText="Codice  Fiscale">
               <VBox.margin>
                  <Insets left="70.0" right="70.0" />
               </VBox.margin>
            </TextField>
         </children>
      </VBox>
      <Separator layoutX="10.0" layoutY="150.0" opacity="0.2" prefWidth="200.0">
         <effect>
            <InnerShadow />
         </effect>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Separator>
      <VBox alignment="TOP_CENTER" prefHeight="160.0" prefWidth="600.0">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets top="10.0" />
         </padding>
         <children>
            <Label text="Dati vaccino">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Label>
            <HBox alignment="TOP_CENTER" prefHeight="61.0" prefWidth="600.0">
               <padding>
                  <Insets top="10.0" />
               </padding>
               <children>
                  <Label styleClass="labelTarget" text="Data somministrazione:">
                     <HBox.margin>
                        <Insets right="10.0" top="5.0" />
                     </HBox.margin>
                  </Label>
                  <DatePicker fx:id="DP_CV_datavaccino" promptText="Inserisci data" />
               </children>
            </HBox>
            <HBox prefHeight="60.0" prefWidth="600.0">
               <padding>
                  <Insets left="100.0" right="100.0" top="15.0" />
               </padding>
               <children>
                  <Label styleClass="labelTarget" text="Tipologia:">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </Label>
                  <RadioButton fx:id="RB_CV_pfizer" mnemonicParsing="false" text="Pfizer">
                     <HBox.margin>
                        <Insets right="15.0" />
                     </HBox.margin>
                     <toggleGroup>
                        <ToggleGroup fx:id="RadioGroup1" />
                     </toggleGroup>
                  </RadioButton>
                  <RadioButton fx:id="RB_CV_astrazeneca" layoutX="134.0" layoutY="20.0" mnemonicParsing="false" prefHeight="19.0" prefWidth="96.0" text="Astrazeneca" toggleGroup="$RadioGroup1">
                     <HBox.margin>
                        <Insets right="15.0" />
                     </HBox.margin>
                  </RadioButton>
                  <RadioButton fx:id="RB_CV_moderna" layoutX="225.0" layoutY="20.0" mnemonicParsing="false" text="Moderna" toggleGroup="$RadioGroup1">
                     <HBox.margin>
                        <Insets right="15.0" />
                     </HBox.margin>
                  </RadioButton>
                  <RadioButton fx:id="RB_CV_jj" layoutX="315.0" layoutY="20.0" mnemonicParsing="false" text="J&amp;J" toggleGroup="$RadioGroup1">
                     <HBox.margin>
                        <Insets right="8.0" />
                     </HBox.margin>
                  </RadioButton>
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox alignment="TOP_CENTER" prefHeight="45.0" prefWidth="600.0">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <children>
            <Label id="L_CV_targetIDvaccino" text="ID univoco:">
               <HBox.margin>
                  <Insets right="10.0" top="5.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="TF_CV_idVaccino">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <Button fx:id="B_CV_confermaRegistrazioneCittadino" mnemonicParsing="false" styleClass="buttonSmall" text="Registra">
         <VBox.margin>
            <Insets left="260.0" right="260.0" />
         </VBox.margin>
      </Button>
      <ProgressIndicator fx:id="CV_registraVaccLoad" prefHeight="27.0" prefWidth="20.0" visible="false">
         <VBox.margin>
            <Insets left="290.0" right="290.0" top="10.0" />
         </VBox.margin>
      </ProgressIndicator>
   </children>
</VBox>
