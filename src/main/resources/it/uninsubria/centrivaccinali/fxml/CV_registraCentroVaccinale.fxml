<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="350.0" prefWidth="800.0" stylesheets="@style/style.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.uninsubria.centrivaccinali.controller.RegistraCV">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER" />
    <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER" />
    <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER" />
    <RowConstraints fillHeight="false" minHeight="10.0" prefHeight="50.0" vgrow="NEVER" />
    <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER" />
    <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER" />
    <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER" />
    <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER" />
  </rowConstraints>
   <children>
      <Label fx:id="L_ttolo" alignment="CENTER" text="Inserisci i dati del centro vaccinale" GridPane.columnSpan="2147483647" GridPane.halignment="CENTER" />
      <TextField fx:id="TF_nome" onKeyTyped="#realtimeCheck" promptText="Inserisci il nome del centro" styleClass="text-field" GridPane.columnSpan="2147483647" GridPane.rowIndex="1" />
      <Label fx:id="L_infoIndirizzo" text="Informazioni indirizzo" GridPane.rowIndex="3" />
      <ChoiceBox fx:id="CB_qualificatore" prefWidth="150.0" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets right="8.0" />
         </GridPane.margin>
      </ChoiceBox>
      <TextField fx:id="TF_indirizzo" onKeyTyped="#realtimeCheck" promptText="indirizzo" GridPane.columnIndex="1" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets left="8.0" right="8.0" />
         </GridPane.margin>
      </TextField>
      <TextField fx:id="TF_civico" onKeyTyped="#realtimeCheck" promptText="civico" GridPane.columnIndex="2" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets left="8.0" right="8.0" />
         </GridPane.margin>
      </TextField>
      <TextField fx:id="TF_comune" onKeyTyped="#realtimeCheck" promptText="comune" GridPane.columnIndex="3" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets left="8.0" right="8.0" />
         </GridPane.margin>
      </TextField>
      <TextField fx:id="TF_provincia" onKeyTyped="#realtimeCheck" promptText="provincia (sigla)" GridPane.columnIndex="4" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets left="8.0" right="8.0" />
         </GridPane.margin>
      </TextField>
      <TextField fx:id="TF_cap" onKeyTyped="#realtimeCheck" promptText="cap" GridPane.columnIndex="5" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets left="8.0" right="8.0" />
         </GridPane.margin>
      </TextField>
      <Label fx:id="L_infoIndirizzo1" text="Seleziona tipologia:" GridPane.rowIndex="6" />
      <RadioButton fx:id="RB_Ospedaliero" mnemonicParsing="false" text="Ospedaliero" GridPane.columnIndex="1" GridPane.rowIndex="6">
         <toggleGroup>
            <ToggleGroup fx:id="TG_tipologia" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="RB_aziendale" mnemonicParsing="false" text="Aziendale" toggleGroup="$TG_tipologia" GridPane.columnIndex="2" GridPane.rowIndex="6" />
      <RadioButton fx:id="RB_hub" mnemonicParsing="false" text="Hub" toggleGroup="$TG_tipologia" GridPane.columnIndex="3" GridPane.rowIndex="6" />
      <Button fx:id="B_indietro" mnemonicParsing="false" text="Indietro" GridPane.halignment="LEFT" GridPane.rowIndex="7" GridPane.valignment="BOTTOM" />
      <Button fx:id="B_conferma" mnemonicParsing="false" onAction="#salvaCentro" text="Conferma" GridPane.columnIndex="5" GridPane.halignment="RIGHT" GridPane.rowIndex="7" GridPane.valignment="BOTTOM" />
      <Separator prefWidth="200.0" GridPane.columnSpan="2147483647" GridPane.rowIndex="2" />
      <Separator prefWidth="200.0" GridPane.columnSpan="2147483647" GridPane.rowIndex="5" />
   </children>
   <padding>
      <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
   </padding>
</GridPane>
