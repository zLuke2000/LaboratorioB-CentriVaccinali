<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import org.kordamp.ikonli.javafx.*?>

<VBox alignment="CENTER" prefHeight="700.0" prefWidth="1200.0" spacing="16.0" styleClass="transparent" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.uninsubria.centrivaccinali.controller.CIDashboardController">
    <stylesheets>
        <URL value="@../../../style/style.css" />
        <URL value="@../../../style/dashboardStyle.css" />
    </stylesheets>
    <padding>
        <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
    </padding>
    <Label styleClass="labelWelcome" text="INFORMAZIONI " textFill="#323232">
        <font>
          <Font name="System Bold" size="30.0" />
        </font>
    </Label>
    <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="8.0" styleClass="opaco-20">
        <Label styleClass="labelTitleSezInfo" text="Dati account" textFill="#323232">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <HBox alignment="CENTER" spacing="8.0">
            <FontIcon iconColor="#323232" iconLiteral="mdi2e-email" iconSize="24" />
            <Label text="Mail:" textFill="#323232" />
            <Label fx:id="CI_L_infoMail" underline="true" wrapText="true" />
        </HBox>
        <HBox alignment="CENTER" spacing="8.0">
            <FontIcon iconColor="#323232" iconLiteral="mdi2a-account" iconSize="24" />
            <Label text="Username:" textFill="#323232" />
            <Label fx:id="CI_L_infoUsername" underline="true" />
        </HBox>
    </VBox>
    <Separator opacity="0.2">
        <effect>
            <InnerShadow />
        </effect>
        <padding>
            <Insets bottom="5.0" top="5.0" />
        </padding>
    </Separator>
    <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="8.0" styleClass="opaco-20">
        <Label styleClass="labelTitleSezInfo" text="Dati Personali" textFill="#323232">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <HBox alignment="CENTER" spacing="8.0">
            <FontIcon iconColor="#323232" iconLiteral="mdi2a-account" iconSize="24" />
            <Label text="Nome:" textFill="#323232" />
            <Label fx:id="CI_L_infoNome" />
        </HBox>
        <HBox alignment="CENTER" spacing="8.0">
            <FontIcon iconColor="#323232" iconLiteral="mdi2a-account" iconSize="24" />
            <Label text="Cognome:" textFill="#323232" />
            <Label fx:id="CI_L_infoCognome" />
        </HBox>
        <HBox alignment="CENTER" spacing="8.0">
            <FontIcon iconColor="#323232" iconLiteral="mdi2i-id-card" iconSize="24" />
            <Label text="Codice Fiscale:" textFill="#323232" />
            <Label fx:id="CI_L_infoCF" />
        </HBox>
        <HBox alignment="CENTER" spacing="8.0">
            <Label text="Id Vaccinazione:" textFill="#323232" />
            <Label fx:id="CI_L_infoIdVacc" />
        </HBox>
    </VBox>
    <Button mnemonicParsing="false" prefHeight="45.0" prefWidth="120.0" styleClass="button-preset-1" text="Chiudi" textFill="#323232" />
</VBox>
