<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="60.0" prefWidth="875.0" stylesheets="@style/style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.uninsubria.centrivaccinali.controller.CIItemListController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" styleClass="style-trasparent">
         <children>
            <ImageView fx:id="CI_IV_TipologiaItem" fitHeight="50.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true" styleClass="style-trasparent" VBox.vgrow="ALWAYS" />
            <Label fx:id="CI_L_TipologiaItem" style="-fx-font-size: 10;" text="Tipologia">
               <font>
                  <Font size="9.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <Label fx:id="CI_L_nomeCVItem" text="Nome">
         <HBox.margin>
            <Insets left="100.0" right="97.0" top="20.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="CI_L_IndirizzoCVItem" text="Indirizzo">
         <HBox.margin>
            <Insets left="100.0" right="97.0" top="20.0" />
         </HBox.margin>
      </Label>
   </children>
</HBox>
