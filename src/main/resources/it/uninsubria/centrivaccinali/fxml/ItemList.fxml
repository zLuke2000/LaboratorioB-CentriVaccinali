<GridPane alignment="TOP_CENTER" prefHeight="67.0" prefWidth="1163.0" stylesheets="@style/style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.uninsubria.centrivaccinali.controller.CIItemListController">
